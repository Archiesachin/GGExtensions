{
    "manifest_version": 3,
    "name": "Web to Phone-Friendly App Converter",
    "version": "1.0",
    "description": "Convert your web app into a phone-friendly app quickly.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }