{
    "manifest_version": 3,
    "name": "W3School Coding Practice",
    "version": "1.0",
    "description": "Navigate to W3School coding practice questions.",
    "action": {
      "default_popup": "welcome.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": [
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  