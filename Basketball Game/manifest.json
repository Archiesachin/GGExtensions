{
"manifest_version": 3,
"name": "Basketball Game Extension",
"version": "1.0",
"description": "Shoot the Ball",
"permissions": [
    "storage"
],
"action": {
    "default_popup": "index.html"
},
"web_accessible_resources": [
    {
        "resources": [
            "index.html"
        ],
        "matches": [
            "<all_urls>"
        ]
    }
]
}