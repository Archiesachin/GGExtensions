{
    "manifest_version": 3,
    "name": "Shoot Duck Game",
    "version": "1.0",
    "description": "A fun Shoot Duck game as a Chrome extension.",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  