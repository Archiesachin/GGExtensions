{
  "manifest_version": 3,
  "name": "Eye Strain Reducer",
  "description": "A simple Chrome extension to reduce eye strain by applying a blue light filter.",
  "version": "1.0",
  "permissions": ["activeTab", "storage", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "512": "icons/2.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
