{
  "manifest_version": 2,
  "name": "Time and Weather New Tab",
  "version": "1.0",
  "description": "Displays the current time and weather on the new tab page.",
  "permissions": ["geolocation", "https://api.openweathermap.org/"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
