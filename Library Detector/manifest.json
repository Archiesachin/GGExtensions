{
    "manifest_version": 3,
    "name": "JS Library Detector",
    "version": "1.0",
    "description": "Detects JavaScript libraries used on websites.",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  