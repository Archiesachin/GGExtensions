{
    "manifest_version": 3,
    "name": "Online Survey and Poll Creator",
    "version": "1.0",
    "description": "Create beautiful and responsive online surveys and polls.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "storage"
    ],
    "icons": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  